{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		// Use SvelteKit default "bundler" as resolution for $lib alias support
		// "module": "NodeNext",
		// "moduleResolution": "NodeNext"
	}
}
